      "type": "n8n-nodes-base.googleSheets",
      "position": [
        -340,
        480
      ],
      "parameters": {
        "range": "=A:B",
        "options": {
          "returnAllMatches": true
        },
        "sheetId": "",
        "operation": "lookup",
        "lookupValue": "={{ $json.output }}",
        "lookupColumn": "Filename",
        "authentication": "oAuth2"
      },
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "",
          "name": "Google Sheets account"
        }
      },
      "notesInFlow": true,
