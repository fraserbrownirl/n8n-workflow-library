      "parameters": {
        "url": "https://www.googleapis.com/drive/v3/channels/stop",
        "method": "POST",
        "options": {},
        "sendBody": true,
        "authentication": "predefinedCredentialType",
        "bodyParameters": {
          "parameters": [
            {
